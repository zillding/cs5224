{
  "name": "cs5224",
  "version": "0.1.0",
  "description": "A simple node server using Koa",
  "scripts": {
    "clean": "rimraf public/static",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "data": "node scripts/parseCSV.js && node scripts/combineJson.js",
    "deploy": "npm run build && cf push",
    "prod": "node lib/index.js",
    "start": "node devServer.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zillding/cs5224.git"
  },
  "author": "Zill Ding",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.6.5",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.1",
    "cross-env": "^1.0.7",
    "eventsource-polyfill": "^0.9.6",
    "fast-csv": "^1.0.0",
    "lodash": "^4.6.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-echarts-component": "^0.1.0",
    "react-redux": "^4.4.1",
    "redux": "^3.3.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "rimraf": "^2.4.3",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.9.1"
  },
  "dependencies": {
    "express": "^4.13.3"
  }
}
